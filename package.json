{
  "name": "pt-inovasi-digital",
  "version": "1.0.0",
  "description": "Company profile website untuk PT. Inovasi Digital Nusantara - Konsultan teknologi terdepan di Indonesia",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "deploy:firebase": "npm run build && firebase deploy",
    "deploy:gh-pages": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  },
  "keywords": [
    "vue3",
    "vite",
    "typescript",
    "pinia",
    "vue-router",
    "supabase",
    "company-profile",
    "indonesia",
    "technology-consulting"
  ],
  "author": {
    "name": "PT. Inovasi Digital Nusantara",
    "email": "tech@inovasidigital.com",
    "url": "https://inovasidigital.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pt-inovasi-digital/company-profile.git"
  },
  "bugs": {
    "url": "https://github.com/pt-inovasi-digital/company-profile/issues"
  },
  "homepage": "https://inovasidigital.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
